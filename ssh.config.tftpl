Host kdcp
    HostName ${cluster_fqdn}
    User ${cluster_user}
    Port 2222

Host kdw1
    HostName ${cluster_name}-worker-01
    User ${cluster_user}
    Port 2222
    ProxyJump kdcp

Host kdw2
    HostName ${cluster_name}-worker-02
    User ${cluster_user}
    Port 2222
    ProxyJump kdcp

Host kdd1
    HostName ${cluster_name}-data-01
    User ${cluster_user}
    Port 2222
    ProxyJump kdcp
